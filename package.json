{
  "name": "@zi0w/calendar-heatmap",
  "version": "0.1.5",
  "description": "React calendar heatmap component with month selector, weekday labels, and legend",
  "license": "MIT",
  "author": "Jiyoung Woo",
  "repository": {
    "type": "git",
    "url": "https://github.com/zi0w/calendar-heatmap.git"
  },
  "keywords": [
    "react",
    "calendar",
    "heatmap",
    "visualization",
    "chart"
  ],
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
