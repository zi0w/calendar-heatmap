{
  "name": "@zi0w/calendar-heatmap",
  "version": "0.1.0",
  "description": "React calendar heatmap component with month selector, weekday labels, and legend",
  "license": "MIT",
  "author": "Jiyoung Woo",
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": ["react", "calendar", "heatmap", "visualization", "chart"],
  "type": "module",
  "sideEffects": false,

  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "dev": "vite",
    "preview": "vite preview",
    "prepublishOnly": "pnpm clean && pnpm typecheck && pnpm build"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tsup": "^8.0.0",
    "typescript": "^5.6.0",
    "vite": "^5.4.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=9"
  }
}
